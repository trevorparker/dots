"""""""""""""
" .vimrc
"""""""""""""

set nocompatible

set ai
set autoindent
set autoread
set background=dark
set clipboard+=unnamed
set cul
set history=300
set hlsearch
set incsearch
set laststatus=2
set number
set paste
set shiftwidth=4
set si
set tabstop=4
set wrap

set ttymouse=xterm2
set mouse=n

syntax enable
